{
  "name": "ts-service-practice",
  "version": "1.0.0",
  "description": "- `npm init`",
  "main": "app.js",
  "dependencies": {
    "@koa/router": "^10.0.0",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "convict": "^6.0.0",
    "joi": "^17.4.0",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "pg": "^8.5.1",
    "supertest": "^6.1.3",
    "typeorm": "^0.2.31",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/convict": "^5.2.2",
    "@types/jest": "^26.0.20",
    "@types/joi": "^17.2.3",
    "@types/koa": "^2.11.7",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa__router": "^8.0.4",
    "@types/node": "^14.14.31",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "npm run migrate && node ./build/src/app.js",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/app.ts",
    "test": "npm run migrate && jest",
    "test:fresh": "npm run reset-db && npm run test",
    "test:dev": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "docker:build": "docker build -t ts-service-practice .",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "migrate": "ts-node ./src/data/migration/migrate.ts",
    "reset-db": "ts-node ./src/data/migration/reset-db.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinindajin/ts-service-practice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pinindajin/ts-service-practice/issues"
  },
  "homepage": "https://github.com/pinindajin/ts-service-practice#readme",
  "jest": {
    "globalSetup": "./test/setup.ts",
    "globalTeardown": "./test/teardown.ts",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
